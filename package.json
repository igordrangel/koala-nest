{
  "name": "@koalarx/nest",
  "version": "1.19.1",
  "description": "",
  "author": "Igor D. Rangel",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "bun": ">=1.0.0"
  },
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "bun run build:app && bun scripts/organize-all-builds.mjs",
    "build:app": "nest build example",
    "build:lib": "nest build koala-nest && bun scripts/organize-all-builds.mjs",
    "build:cli": "node scripts/build-cli.mjs",
    "build:mcp": "cd apps/mcp-server && tsc",
    "build:mcp-extension": "tsc -p apps/mcp-vscode-extension && cp apps/mcp-server/dist/server.js apps/mcp-vscode-extension/dist/server.js",
    "build:mcp-all": "bun run build:mcp && bun run build:mcp-extension",
    "build:publish": "bun .github/scripts/publish.mjs",
    "package:vscode-extension": "bun scripts/package-vscode-extension.mjs",
    "watch:mcp-extension": "tsc -w -p apps/mcp-vscode-extension",
    "test:mcp-extension": "bun apps/mcp-vscode-extension/dist/test/runTest.js",
    "start": "nest start example",
    "start:dev": "nest start example --watch",
    "start:debug": "nest start example --debug --watch",
    "start:mcp": "bun apps/mcp-server/dist/server.js",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:cov": "bun test --coverage",
    "test:debug": "bun test --inspect-brk",
    "test:e2e": "bun test --preload apps/example/src/test/setup-e2e.ts --testNamePattern=e2e",
    "test:all": "bun run test && bun run test:e2e",
    "prepublishOnly": "bun run test",
    "postversion": "git push && git push --tags",
    "deploy:hotfix": "bun pm version patch",
    "deploy:feature": "bun pm version minor",
    "deploy:release": "bun pm version major",
    "changeset": "changeset",
    "changeset:add": "changeset add",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "sync:docs": "cp docs/*.md apps/koala-nest-docs/",
    "dev:docs": "bun run sync:docs && cd apps/koala-nest-docs && bun run dev",
    "build:docs": "bun run sync:docs && cd apps/koala-nest-docs && bun run build",
    "preview:docs": "cd apps/koala-nest-docs && bun run preview"
  },
  "dependencies": {
    "@koalarx/utils": "^4.2.1",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.12",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/swagger": "^11.0.7",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@scalar/nestjs-api-reference": "^0.4.3",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "consola": "^3.4.2",
    "dotenv": "^16.0.3",
    "express-basic-auth": "^1.2.1",
    "inquirer": "^6.2.2",
    "ioredis": "^5.3.2",
    "ngrok": "^5.0.0-beta.2",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "zod": "^3.22.4",
    "zod-validation-error": "^1.5.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.23.0",
    "@faker-js/faker": "^9.6.0",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^11.0.12",
    "@types/bun": "^1.3.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.11",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.2",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/eslint-plugin": "^1.6.6",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.0.0",
    "prettier": "^3.8.0",
    "prisma": "^7.2.0",
    "source-map-support": "^0.5.20",
    "supertest": "^7.1.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  }
}
